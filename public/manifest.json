{
  "manifest_version": 3,
  "description": "A chrome extension",
  "version": "1.0.0",
  "name": "Create React App Sample",
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "/static/js/background.js"
  },
  "action": {
    "default_title": "Open the popup",
    "default_icon":{
      "16":"logo192.png",
      "128":"logo512.png"
    }
  }
}
